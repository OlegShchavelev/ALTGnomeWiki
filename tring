[32m+[m
[32m+[m[32mconst { theme } = useData()[m
[32m+[m
[32m+[m[32mconst transformedApps = computed(() => {[m
[32m+[m[32m  const meta = theme.value.meta[m
[32m+[m
[32m+[m[32m  return apps.map((app: any) => ({[m
[32m+[m[32m    ...app,[m
[32m+[m[32m    name: app.appstream.name,[m
[32m+[m[32m    icon: app.appstream.icon,[m
[32m+[m[32m    summary: app.appstream.summary,[m
[32m+[m[32m    keywords: app.appstream.keywords?.map((keyword: string) => {[m
[32m+[m[32m      const types = meta.keywords[m
[32m+[m[32m      return {[m
[32m+[m[32m        name: keyword,[m
[32m+[m[32m        type: types[keyword] ?? 'tip'[m
[32m+[m[32m      }[m
[32m+[m[32m    }),[m
[32m+[m[32m    actions: Object.entries(app.aggregation ?? {})[m
[32m+[m[32m      .filter(([key]) => meta.actions?.[key])[m
[32m+[m[32m      .map(([key, value]) => {[m
[32m+[m[32m        const baseValue =[m
[32m+[m[32m          typeof value === 'object' && value !== null[m
[32m+[m[32m            ? value[m
[32m+[m[32m            : { id: typeof value === 'string' ? value : undefined }[m
[32m+[m
[32m+[m[32m        const { id = key, ...extraProps } = baseValue as { id?: string }[m
[32m+[m
[32m+[m[32m        return {[m
[32m+[m[32m          id,[m
[32m+[m[32m          text: key,[m
[32m+[m[32m          ...(meta.actions[key] ?? {}),[m
[32m+[m[32m          ...extraProps[m
[32m+[m[32m        }[m
[32m+[m[32m      })[m
[32m+[m[32m  }))[m
[32m+[m[32m})[m
